#include <stdio.h>
#include <math.h>

double piA(double e);

double piA(double e){
  return pi(start,e);
}

double pi(double a, double e){
  double temp = 2*a / sqrt(a);
  return temp:
}


int main(void){
  double start = 3;
  double e = 0.0000001;
  printf("Ergebnis: %d\n", piA(e));
  
